        pause(1000)\n            if (Math.percentChance(50) && pain_elemental_hp.value > 0) {\n                pain_elemental.follow(me, 0)\n                lost_soul = sprites.createProjectileFromSprite(img`\n                    . 4 . . . . . . 4 . . . . . . \n                    . . 4 4 . . . . 4 . . . 4 . . \n                    . . . b 4 . . . 4 4 . b . . . \n                    . . . b 4 . 4 4 4 4 . b . . . \n                    . . b b 4 4 2 4 4 4 4 b b . . \n                    . . b b b d d d d d b b b . . \n                    . . 4 b d d d d d d d b 4 . . \n                    . . 4 4 d f f d f f d 4 4 . . \n                    . . 4 4 d f 2 d 2 f d 4 4 . . \n                    . . 4 4 d d d d d d d 4 4 . . \n                    . . . . 4 d d d d d 4 4 . . . \n                    . . . . 4 4 d 4 d 4 4 4 . . . \n                    . . . . . d 4 d 4 d 4 4 . . . \n                    . . . . . d d d d d 4 . . . . \n                    . . . . . . . . . . . . . . . \n                    . . . . . . . . . . . . . . . \n                    `, pain_elemental, 0, 0)\n                lost_soul_hp = statusbars.create(20, 4, StatusBarKind.Health)\n                lost_soul_hp.attachToSprite(lost_soul)\n                lost_soul.setKind(SpriteKind.imp)\n                lost_soul.follow(me, 50)\n                timer.after(2000, function () {\n                    pain_elemental.follow(me, 15)\n                })\n            }\n        }\n    }\n}\nsprites.onOverlap(SpriteKind.imp, SpriteKind.Player, function (sprite, otherSprite) {\n    timer.throttle(\"damage\", 2000, function () {\n        healthbar_hud.value += -20\n        death()\n    })\n})\nsprites.onOverlap(SpriteKind.Projectile, SpriteKind.imp, function (sprite, otherSprite) {\n    sprites.destroy(sprite)\n    otherSprite.follow(me, 0)\n    otherSprite.setVelocity(Render.getAttribute(Render.attribute.dirX) + 10, Render.getAttribute(Render.attribute.dirY) + 10)\n    pause(200)\n    otherSprite.setVelocity(0,